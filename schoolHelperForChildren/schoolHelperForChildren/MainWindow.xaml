<Window x:Class="schoolHelperForChildren.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:schoolHelperForChildren"
        mc:Ignorable="d"
        Title="MainWindow" Height="640" Width="940">
    <Window.Background>
        <ImageBrush ImageSource="Images\blackBoard.png" />
    </Window.Background>
    <Grid HorizontalAlignment="Left" Height="280" Margin="22,304,0,0" VerticalAlignment="Top" Width="885">
        <TextBox x:Name="nameTxtbox" HorizontalAlignment="Left" Height="53" Margin="232,79,0,0" TextWrapping="Wrap" Text="TextBox" VerticalAlignment="Top" Width="410" GotFocus="nameTxtbox_GotFocus"/>
        <Button x:Name="nameBtn" Content="Submit Name" HorizontalAlignment="Left" Margin="369,137,0,0" VerticalAlignment="Top" Width="121" Height="62"/>
    </Grid>


</Window>
